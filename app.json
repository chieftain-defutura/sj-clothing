{
  "expo": {
    "name": "Sprinkle Nadar",
    "scheme": "sjclothing",
    "slug": "sj-clothing",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/notificationIcon.png",
      "color": "#311b92",
      "androidMode": "default",
      "androidCollapsedTitle": "Sprinkle Nadar",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/iPhone.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.dewallstreet.sprinkle",
      "config": {
        "googleMapsApiKey": "AIzaSyDgFFGbaEKBnJJT73Ewzftdg0tljD_ZA9c"
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDgFFGbaEKBnJJT73Ewzftdg0tljD_ZA9c"
        }
      },
      "package": "com.dewallstreet.sprinklenadar",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Sprinkle Nadar to use your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.sjclothing",
          "enableGooglePay": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationIcon.png",
          "color": "#CA2C92",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Sprinkle Nadar",
          "iosDisplayInForeground": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "78d85b5a-8351-42b5-a5a6-6937fb28159c"
      }
    }
  }
}
