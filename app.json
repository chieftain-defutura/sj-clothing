        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.braj1410.Sprinkle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3f866d31-5243-45d6-b35e-7dcc398752d5"
      }
    },
    "plugins": ["expo-localization"]
  }
}
